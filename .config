# Target system
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_generic=y

# Build options
CONFIG_BUILD_PATENTED=n
CONFIG_BUILD_LOG=y

# Toolchain options
CONFIG_TOOLCHAIN_BUILDROOT_GLIBC=y
CONFIG_TOOLCHAIN_CXX=y

# Base system options
CONFIG_PACKAGE_busybox=y
CONFIG_PACKAGE_opkg=y

# Kernel modules (you may want to customize this)
CONFIG_KERNEL_KMOD=y

# Enable your luci app as module
CONFIG_PACKAGE_luci-app-routewatch=m
CONFIG_DEVEL=y
CONFIG_BUILD_NPKG=y
CONFIG_PKG_USE_DEB_IPK=y